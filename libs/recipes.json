[
  {
    "id": "2c67c141-d86f-472f-8570-e5db295011a6",
    "title": "Lasagna 1",
    "servings": 2,
    "time": 50,
    "image": "assets/lasagna.jpg",
    "ingredients": [
      {
        "id": "957051ff-2614-427c-955e-af741045ae8a",
        "name": "Lasagna Noodles",
        "type": "Pasta",
        "quantity": { "number": 12, "unit": "unit" }
      },
      {
        "id": "70ca0835-d631-49b5-a358-f62f1f8393cf",
        "name": "Brown Button Mushroom",
        "type": "Sauce",
        "quantity": { "number": 225, "unit": "grammes" }
      },
      {
        "id": "7a65b88b-fcf3-42c5-8527-7892be4ba37b",
        "name": "Onion",
        "type": "Vegetables",
        "quantity": { "number": 1, "unit": "unit" }
      },
      {
        "id": "a1c8561c-d89c-4468-a29b-08e2ddfcbdf9",
        "name": "Clove Garlic",
        "type": "Herb",
        "quantity": { "number": 2, "unit": "unit" }
      },
      {
        "id": "b3f08cf4-2e4e-4d76-b3a6-6f0c1689e47d",
        "name": "Olive Oil",
        "type": "Oil",
        "quantity": { "number": 2, "unit": "table spoons" }
      },
      {
        "id": "adf2ecdb-89a5-4890-9c32-d9cdd857768b",
        "name": "Bolognese Sauce",
        "type": "Sauce",
        "quantity": { "number": 5, "unit": "cups" }
      },
      {
        "id": "5510d212-7630-4df7-904f-3fb1bd46dc72",
        "name": "Shredded Mozarella",
        "type": "Cheese",
        "quantity": { "number": 4, "unit": "cups" }
      },
      {
        "id": "737e8592-b0b6-45b4-afaa-a690f26415fd",
        "name": "Salt",
        "type": "Seasoning",
        "quantity": { "number": 50, "unit": "grammes" }
      }
    ],
    "preparation": [
      {
        "step_number": 1,
        "description": "Cook the pasta in boiling salted water until al dente. Rinse under cold water and oil lightly. Set aside."
      },
      {
        "step_number": 2,
        "description": "In a skillet, brown the mushrooms, onion and garlic in the oil approximately 10 minutes. Season with salt and pepper. Deglaze with the vinegar and water. Adjust seasoning. Set aside."
      },
      {
        "step_number": 3,
        "description": "With the rack in the middle position, preheat the oven to 190 °C (375 °F)."
      },
      {
        "step_number": 4,
        "description": "In a 33 x 23-cm (13 x 9-inch) baking dish, spread a quarter of the sauce. Cover with a layer of pasta. Add another quarter of the sauce and 150 ml (2/3 cup) of cheese. Cover with another layer of pasta. Add the mushroom mixture and 150 ml (2/3 cup) of cheese. Cover with a layer of pasta. Add a quarter of the sauce and 150 ml (2/3 cup) of cheese. Finish with the remaining noodles, sauce and cheese. Bake for approximately 45 minutes then brown under the broiler. Let stand for 5 minutes before serving."
      }
    ]
  },
